<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>Oarlok</display-name>  
  <filter>
    <filter-name>DispatcherFilter</filter-name>
    <filter-class>oarlok.dispatcher.DispatchFilter.DispatcherFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>DispatcherFilter</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>
  <error-page>
    <error-code>403</error-code>
    <location>/error.jsp</location>
  </error-page>
  <servlet>
    <description></description>
    <display-name>LoginResource</display-name>
    <servlet-name>LoginResource</servlet-name>
    <servlet-class>oarlok.application.resources.LoginResource</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LoginResource</servlet-name>
    <url-pattern>/Login</url-pattern>
  </servlet-mapping>
</web-app>